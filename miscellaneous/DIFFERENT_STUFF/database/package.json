{
  "name": "medmo-db",
  "version": "1.0.0",
  "scripts": {
    "knex": "node ./node_modules/knex/bin/cli.js --knexfile ./knexfile.js",
    "knex:migrate:dev": "node ./node_modules/knex/bin/cli.js --knexfile ./knexfile.js migrate:latest --env development",
    "knex:migrate:prod": "node ./node_modules/knex/bin/cli.js --knexfile ./knexfile.js migrate:latest --env production",
    "knex:seed:run": "node ./node_modules/knex/bin/cli.js --knexfile ./knexfile.js seed:run",
    "generate:seed": "node ./seedGenerator.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "knex": "^0.95.6",
    "mysql2": "^2.1.0",
    "objection": "^2.2.15"
  },
  "private": true
}
